import os

os.system("cls || clear")

numeros = []
valorNegativo = 0

for i in range(5):
    numero = int(input(f"Digite o {i+1}º número: "))
    numeros.append(numero)

    if(numero < 0):
        numeros[i] *= 0
    
os.system("cls || clear")    
for i in range(len(numeros)):
    print(f"Números Inseridos: {numeros[i]}")

